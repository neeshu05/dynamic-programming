class Solution:
    def distinctSequences(self, n: int) -> int:
        m = [[1,2,3,4,5,6], 
             [1,2,3,4,5,6],
             [1,2,3,4,5,6],
             [1,2,3,4,5,6],
             [1,2,3,4,5,6],
             [1,2,3,4,5,6],
             [1,2,3,4,5,6]]
        
        mod = (10 ** 9) + 7
        @cache
        def dp(prev, n):
            if n == 0: return 1
            return sum(dp(x, n - 1) for x in m[prev]) % mod
        return dp(0, n)
